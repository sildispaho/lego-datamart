                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH child_counts AS
(
SELECT
	parent_part_num,
	COUNT(*) AS Nr_of_Children
FROM part_relationships
GROUP BY parent_part_num
)

SELECT
	pr.parent_part_num AS Part_Num,
	p.name AS Part_Name,
	ip.color_id AS Color_ID,
	p.part_cat_id AS Category_ID,
	SUM(TRY_CAST(ip.quantity AS INT)) AS Nr_of_Parts,
	cc.Nr_of_Children
FROM inventory_parts ip

JOIN parts p
ON ip.part_num = p.part_num
JOIN part_relationships pr
ON ip.part_num = pr.child_part_num
JOIN child_counts cc
ON pr.parent_part_num = cc.parent_part_num

